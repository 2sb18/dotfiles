loadplugins '\.(js|penta)$'
group user


" taken from Rizal Almashoor {{{
set hintkeys=sadfjklewcmpgh
highlight -a Hint padding: 0 2px 0 1px; text-transform: lowercase; background-color: yellow; font: bold 14px "Droid Sans Mono";
highlight -a HintElem background-color: transparent;

" tab numbering
set go+=n
" }}}

" Gmail shortcuts should work
" taken from https://mail.google.com/mail/u/0/?pli=1#inbox
set passkeys+=mail.google.com:'!#*+-./<C-Down><C-s><Enter>?INU[]acefjk lmnopqrsuvxyz',*a,*n,*r,*s,*t,*u,ga,gc,gd,gi,gl,gs,gt,yo
set strictfocus+=mail.google.com:laissez-faire
" }}}


